# Provider's EDC Connector Endpoint
BASE_URL=https://your-provider-edc-connector.domain.com/api/v1/management
API_KEY=YOUR_PROVIDER_EDC_API_KEY

# Default asset name to use if not specified via CLI/param
DEFAULT_ASSET_NAME=default-provider-asset

# Logging and Response Printing (optional, true/false)
PRINT_RESPONSE=false
PRINT_FIRST_JSON_ELEMENT_ONLY=true
LOG_LEVEL=INFO # Supported: DEBUG, INFO, WARNING, ERROR, CRITICAL

# Business Partner Number of this provider
PROVIDER_BPN=BPNL0000000YOURPROVIDER

# Business Partner Number of the target consumer for policy creation
CONSUMER_BPN=BPNL0000000YOURCONSUMER

# S3 Configuration for Data Storage
S3_ENDPOINT=your-s3-compatible-endpoint.com # e.g., minio.example.com or s3.amazonaws.com
S3_ACCESS_KEY=YOUR_S3_ACCESS_KEY
S3_SECRET_KEY=YOUR_S3_SECRET_KEY
S3_SECURE=true # Use true for HTTPS, false for HTTP
S3_REGION=your-s3-region # e.g., us-east-1, eu-central-1

# Default bucket name for S3 storage where assets will be uploaded
DEFAULT_BUCKET_NAME=your-default-s3-bucket-name